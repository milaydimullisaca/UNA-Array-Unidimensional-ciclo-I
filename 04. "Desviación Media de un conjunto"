#include <iostream>
#include<cmath>
using namespace std;
int main() {
    float suma,dm,n,val,x,y;
    suma=0;
    cin>>n;
   float num[5]={10,7,8,10,6};
      for(int i=0;i<n;i++){
         suma=suma+num[i];
         x=suma
}
     y=x/n;
    for(int i=0;i<n;i++){
        val=val+abs(num[i]-y);
    }
    dm=val/n;
    cout<<"la desviacion media es:"<<dm;
    return 0;
}
